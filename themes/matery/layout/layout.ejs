            if (windowWidth > 992) {
                document.write('<script type="text/javascript" size="<%- theme.ribbon.size %>" alpha="<%- theme.ribbon.alpha %>" zIndex="<%- theme.ribbon.zIndex %>" src="<%- theme.libs.js.ribbon %>"><\/script>');
            }
